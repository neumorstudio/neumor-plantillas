{
  "name": "neumorstudio",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
  "scripts": {
    "dev": "turbo dev",
    "dev:web": "turbo dev --filter=@neumorstudio/template-restaurant --filter=@neumorstudio/admin",
    "dev:admin": "turbo dev --filter=@neumorstudio/admin",
    "dev:restaurant": "turbo dev --filter=@neumorstudio/template-restaurant",
    "dev:salon": "turbo dev --filter=@neumorstudio/template-salon",
    "dev:clinic": "turbo dev --filter=@neumorstudio/template-clinic",
    "dev:gym": "turbo dev --filter=@neumorstudio/template-gym",
    "dev:store": "turbo dev --filter=@neumorstudio/template-store",
    "dev:docs": "pnpm --filter=@neumorstudio/docs dev",
    "local": "turbo dev --filter=@neumorstudio/admin --filter=@neumorstudio/template-unified",
    "build": "turbo build",
    "build:docs": "pnpm --filter=@neumorstudio/docs build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "typecheck": "pnpm type-check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "create-client": "pnpm --filter=@neumorstudio/cli create-client",
    "serve:docs": "pnpm --filter=@neumorstudio/docs serve"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.0.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@supabase/ssr": "^0.5.2",
    "lucide-react": "^0.562.0"
  }
}
